{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "richtext"
    },
    "price": {
      "type": "decimal"
    },
    "imageUrl": {
      "type": "string"
    },
    "imageAlt": {
      "type": "string"
    },
    "gallery": {
      "type": "media",
      "multiple": true
    },
    "material": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::material.material"
    },
    "occasions": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::occasion.occasion"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category"
    },
    "customizable": {
      "type": "boolean",
      "default": false
    },
    "sizes": {
      "type": "json"
    },
    "inStock": {
      "type": "boolean",
      "default": true
    },
    "rating": {
      "type": "decimal"
    },
    "reviewCount": {
      "type": "integer"
    }
  }
}
